# All calls to awk removed, because Minix bawk is deficient.  (kjb)

exec > operators.c
echo '/*
 * Operators used in the expr/test command.
 */

#include "../shell.h"
#include "operators.h"

char *const unary_op[] = {'
sed -e '/^[^#]/!d
	s/[ 	][ 	]*/ /g
	s/^[^ ][^ ]* \([^ ][^ ]*\).*/      "\1",/
	' unary_op
echo '      NULL
};

char *const binary_op[] = {'
sed -e '/^[^#]/!d
	s/[ 	][ 	]*/ /g
	s/^[^ ][^ ]* \([^ ][^ ]*\).*/      "\1",/
	' binary_op
echo '      NULL
};

const char op_priority[] = {'
sed -e '/^[^#]/!d
	s/[ 	][ 	]*/ /g
	s/^[^ ][^ ]* [^ ][^ ]* \([^ ][^ ]*\).*/      \1,/
	' unary_op binary_op
echo '};

const char op_argflag[] = {'
sed -e '/^[^#]/!d
	s/[ 	][ 	]*/ /g
	s/^[^ ][^ ]* [^ ][^ ]* [^ ][^ ]*$/& 0/
	s/^[^ ][^ ]* [^ ][^ ]* [^ ][^ ]* \([^ ][^ ]*\)/      \1,/
	' unary_op binary_op
echo '};'
